{
  "name": "collectfi-api",
  "version": "1.0.0",
  "description": "API service for fractionalized collectibles trading on Solana blockchain",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist coverage",
    "example": "ts-node examples/basic-usage.ts",
    "example:build": "tsc examples/basic-usage.ts --outDir dist/examples --target es2020 --module commonjs",
    "example:api": "node examples/api-usage.js",
    "dev:watch": "nodemon src/index.ts"
  },
  "keywords": [
    "solana",
    "blockchain",
    "collectibles",
    "fractionalization",
    "spl-tokens",
    "web3",
    "collectfi",
    "rolex",
    "luxury-watches",
    "daytona",
    "trading",
    "api",
    "rest-api"
  ],
  "author": "CollectFi Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/collectfi/collectfi-api.git"
  },
  "bugs": {
    "url": "https://github.com/collectfi/collectfi-api/issues"
  },
  "homepage": "https://github.com/collectfi/collectfi-api#readme",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
